{
	"name": "11ty-newspaper",
	"version": "9.0.0",
	"description": "A starter repository for a blog newspaper web site using the Eleventy site generator developed by Adam Dj Brett.",
	"type": "module",
	"scripts": {
		"build": "npx @11ty/eleventy && npx pagefind --site _site",
		"build-nocolor": "cross-env NODE_DISABLE_COLORS=1 npx @11ty/eleventy",
		"build-ghpages": "npx @11ty/eleventy --pathprefix=/11ty-newspaper/",
		"start": "npx @11ty/eleventy --serve --quiet",
		"start-ghpages": "npx @11ty/eleventy --pathprefix=/11ty-newspaper/ --serve --quiet",
		"debug": "cross-env DEBUG=Eleventy* npx @11ty/eleventy",
		"debugstart": "cross-env DEBUG=Eleventy* npx @11ty/eleventy --serve --quiet",
		"benchmark": "cross-env DEBUG=Eleventy:Benchmark* npx @11ty/eleventy"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/11ty/11ty-newspaper.git"
	},
	"author": {
		"name": "Adam Dj Brett",
		"email": "info@adamdjbrett.com",
		"url": "https://adamdjbrett.com/"
	},
	"license": "MIT",
	"engines": {
		"node": ">=20"
	},
	"devDependencies": {
		"@11ty/eleventy": "^3.1.2",
		"@11ty/eleventy-img": "^6.0.4",
		"@11ty/eleventy-navigation": "^1.0.5",
		"@11ty/eleventy-plugin-rss": "^2.0.4",
		"@11ty/eleventy-plugin-syntaxhighlight": "^5.0.2",
		"cross-env": "^10.1.0",
		"luxon": "^3.7.2",
		"markdown-it": "^14.1.0",
		"markdown-it-attrs": "^4.3.1",
		"prismjs": "^1.30.0",
		"zod": "^4.3.6",
		"zod-validation-error": "^5.0.0"
	},
	"dependencies": {
		"@zachleat/heading-anchors": "^1.0.5",
		"bootstrap": "^5.3.8",
		"cheerio": "1.0.0-rc.12",
		"eleventy-plugin-toc": "^1.1.5",
		"markdown-it": "^14.1.0",
		"markdown-it-anchor": "^9.2.0",
		"markdown-it-footnote": "^4.0.0",
		"markdown-it-table-of-contents": "^0.8.0",
		"pagefind": "^1.4.0",
		"simpledotcss": "^2.3.7",
		"slugify": "^1.6.6"
	}
}
